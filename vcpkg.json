{
  "name": "3dtiles",
  "version-string": "1.0.0",
  "description": "A 3D Tiles generation library",
  "dependencies": [
    {
      "name": "osg",
      "default-features": true,
      "platform": "!osx"
    },
    {
      "name": "osg",
      "default-features": false,
      "features": ["plugins"],
      "platform": "osx"
    },
    {
      "name": "gdal",
      "default-features": false,
      "features": ["geos", "gif", "iconv", "jpeg", "libkml", "libspatialite", "openjpeg", "png", "recommended-features" ,"sqlite3", "webp", "zstd"]
    },
    "basisu",
    "draco",
    "meshoptimizer"
  ],
  "overrides": [
    {
      "name": "gdal",
      "version": "3.8.4"
    }
  ],
  "builtin-baseline": "d5182f703b51d7b258f83f94d936ac03488dcdbe"
}